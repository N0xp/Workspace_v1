<node name="move_base" pkg="move_base" type="move_base" respawn="false" output="screen">
    <!-- <param name="base_global_planner" type="string" value="global_planner/GlobalPlanner" /> -->
    <param name="base_global_planner" value="navfn/NavfnROS"/>
    <param name="base_local_planner" value="base_local_planner/TrajectoryPlannerROS"/>
    <param name="simple_goal_type" value="geometry_msgs/PoseStamped"/>
    <param name="simple_goal_tolerance" value="0.2"/>
    <param name="goal_tolerance" value="0.2"/>
    <!-- <param name="base_local_planner" value="teb_local_planner/TebLocalPlannerROS" /> -->
    <rosparam file="$(find robot_discription)/config/costmap_common_params.yaml" command="load" ns="global_costmap" />
    <rosparam file="$(find robot_discription)/config/costmap_common_params.yaml" command="load" ns="local_costmap" />
    <rosparam file="$(find robot_discription)/config/local_costmap_params.yaml" command="load" />
    <rosparam file="$(find robot_discription)/config/global_costmap_params.yaml" command="load" />
    <rosparam file="$(find robot_discription)/config/base_local_planner_params.yaml" command="load" />

    <param name="controller_frequency" value="30.0" />
    <param name="planner_frequency" value="10.0" />
  </node>


 <node pkg="vmxpi_ros_bringup" type="drive_keyboard_teleop_node" name="drive_keyboard_teleop_node" output="screen" />
    <node pkg="rviz" type="rviz" name="rviz" args = "-d $(arg rviz_config_file)"/>
    

</launch>